<?xml version="1.0" encoding="UTF-8"?>
<Configuartion status="WARN">
	<Appenders>
		<Console name = "STDOUT" target="SYSTEM_OUT">
			<PatternLayout pattern = "class:%C{3} method:%M line:%L message:%m%n"/>
		</Console>
	</Appenders>
	<Loggers>
		<Logger name="Test" level="debug" additivity="false">
			<AppenderRef ref="STDOUT"/>
		</Logger>
		<Root level="debug">
			<AppenderRef ref="STDOUT"/>
		</Root>
	</Loggers>
</Configuartion>